{
  "crons": [
    {
      "path": "/api/cron/scrape",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/analyze",
      "schedule": "30 * * * *"
    }
  ],
  "functions": {
    "app/api/cron/scrape/route.ts": {
      "memory": 1024,
      "maxDuration": 300
    },
    "app/api/cron/analyze/route.ts": {
      "memory": 1024,
      "maxDuration": 300
    },
    "app/api/projects/*/scrape/route.ts": {
      "memory": 1024,
      "maxDuration": 300
    }
  }
}
